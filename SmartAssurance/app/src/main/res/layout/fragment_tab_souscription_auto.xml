<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:orientation="vertical"
    android:padding="10dp"
    android:weightSum="1"
    tools:context="com.aro.misaina.smartassurance.TabInfoSousAutoFragment">

    <!-- TODO: Update blank fragment layout -->
    <TextView
        android:id="@+id/vTitre"
        style="@style/TextNormal"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_weight="0.05"
        android:gravity="fill_vertical"
        android:text="Info" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.05"
        android:orientation="vertical"
        android:weightSum="1">

        <TextView
            android:id="@+id/textView14"
            style="@style/TextNormal"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="fill_vertical"
            android:text="Souscripteur" />

        <TextView
            android:id="@+id/vSouscripteur"
            style="@style/TextNormal"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="fill_vertical"
            android:text="TextView" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.05"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView6"
            style="@style/TextNormal"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="fill_vertical"
            android:text="Date de souscription" />

        <TextView
            android:id="@+id/vDateDebut"
            style="@style/TextNormal"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:gravity="fill_vertical" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.05"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView11"
            style="@style/TextNormal"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:gravity="fill_vertical"
            android:text="Durée" />

        <TextView
            android:id="@+id/vDuree"
            style="@style/TextNormal"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:gravity="fill_vertical"
            android:text="TextView" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.05"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView9"
            style="@style/TextNormal"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:gravity="fill_vertical"
            android:text="N° police" />

        <TextView
            android:id="@+id/vNopolice"
            style="@style/TextNormal"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:gravity="fill_vertical"
            android:text="TextView" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.05"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView16"
            style="@style/TextNormal"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:gravity="fill_vertical"
            android:text="Prime net" />

        <TextView
            android:id="@+id/vPrimeNet"
            style="@style/TextNormal"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:gravity="fill_vertical" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.05"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView18"
            style="@style/TextNormal"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:gravity="fill_vertical"
            android:text="Prime total" />

        <TextView
            android:id="@+id/vPrimeTot"
            style="@style/TextNormal"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:gravity="fill_vertical"
            android:text="TextView" />
    </LinearLayout>

</LinearLayout>
