<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="10dp"
    tools:context="com.aro.misaina.smartassurance.TabInfoSinistreFragment">

    <!-- TODO: Update blank fragment layout -->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tConducteur"
            style="@style/TextNormal"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="Conducteur" />

        <TextView
            android:id="@+id/tNom"
            style="@style/TextNormal"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="TextView" />

        <TextView
            android:id="@+id/tPrenom"
            style="@style/TextNormal"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="TextView" />

        <TextView
            android:id="@+id/tDn"
            style="@style/TextNormal"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="TextView" />

        <TextView
            android:id="@+id/tAdresse"
            style="@style/TextNormal"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="TextView" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView37"
            style="@style/TextNormal"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="Permis" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                style="@style/TextNormal"
                android:layout_width="10dp"
                android:layout_height="30dp"
                android:layout_weight="0.64"
                android:text="N°" />

            <TextView
                android:id="@+id/tNo"
                style="@style/TextNormal"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="TextView" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView42"
                style="@style/TextNormal"
                android:layout_width="10dp"
                android:layout_height="30dp"
                android:layout_weight="0.64"
                android:text="Duplicata N°" />

            <TextView
                android:id="@+id/tDupl"
                style="@style/TextNormal"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="TextView" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView45"
                style="@style/TextNormal"
                android:layout_width="10dp"
                android:layout_height="30dp"
                android:layout_weight="0.64"
                android:text="Catégories" />

            <TextView
                android:id="@+id/tCats"
                style="@style/TextNormal"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="TextView" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView47"
                style="@style/TextNormal"
                android:layout_width="10dp"
                android:layout_height="30dp"
                android:layout_weight="0.64"
                android:text="Cat. validées" />

            <TextView
                android:id="@+id/tCatValidees"
                style="@style/TextNormal"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="TextView" />
        </LinearLayout>

        <TextView
            android:id="@+id/textView48"
            style="@style/TextNormal"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="Validité" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tValDeb"
                style="@style/TextNormal"
                android:layout_width="10dp"
                android:layout_height="30dp"
                android:layout_weight="0.64"
                android:text="TextView" />

            <TextView
                android:id="@+id/tValFin"
                style="@style/TextNormal"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="TextView" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textView52"
                style="@style/TextNormal"
                android:layout_width="10dp"
                android:layout_height="30dp"
                android:layout_weight="0.64"
                android:text="Date de délivrance" />

            <TextView
                android:id="@+id/tDateDeliv"
                style="@style/TextNormal"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="TextView" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textView54"
                style="@style/TextNormal"
                android:layout_width="10dp"
                android:layout_height="30dp"
                android:layout_weight="0.65"
                android:text="Lieu de délivrance" />

            <TextView
                android:id="@+id/tLieuDeliv"
                style="@style/TextNormal"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:text="TextView" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
